    "test-build": "parcel build ./src/js/index.js --out-dir ./testing --out-file index.js --no-autoinstall --no-source-maps"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.5.1",
    "sass": "^1.32.5"
  },
  "staticFiles": {
    "staticPath": [
      "./src"
    ],
    "watcherGlob": "**",
    "excludeGlob": [
      "js",
      "js/**",
      "scss",
      "scss/**"
    ]
  }
